<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <display-name>UPCDEW_TaxiEmpresarial_ITaxi_Maven_</display-name>
    
    
    
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>pe.edu.upc.dew.taxiseguro.controller.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PortalClienteservlet</servlet-name>
        <servlet-class>pe.edu.upc.dew.taxiseguro.controller.PortalClienteservlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginClienteServlet</servlet-name>
        <servlet-class>pe.edu.upc.dew.taxiseguro.controller.LoginClienteServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ConsultaServlet</servlet-name>
        <servlet-class>pe.edu.upc.dew.taxiseguro.controller.ConsultaServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ConfirmaReserva</servlet-name>
        <servlet-class>pe.edu.upc.dew.taxiseguro.controller.ConfirmaReservaServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ConsultaDisponibilidadServlet</servlet-name>
        <servlet-class>pe.edu.upc.dew.taxiseguro.controller.ConsultaDisponibilidadServlet</servlet-class>
    </servlet>



    
   <servlet>
    <servlet-name>action</servlet-name>
    <servlet-class>
        org.apache.struts.action.ActionServlet
    </servlet-class>
    <init-param>
        <param-name>config</param-name>
        <param-value>
         /WEB-INF/struts-config.xml
        </param-value>
    </init-param>
    <load-on-startup>1</load-on-startup>
  </servlet>
 
  <servlet-mapping>
       <servlet-name>action</servlet-name>
       <url-pattern>*.do</url-pattern>
  </servlet-mapping>
 
    
   
    <servlet-mapping>
        <servlet-name>LoginClienteServlet</servlet-name>
        <url-pattern>/portal</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/PortaCliente</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PortalClienteservlet</servlet-name>
        <url-pattern>/sedeServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ConsultaServlet</servlet-name>
        <url-pattern>/Consulta</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ConfirmaReserva</servlet-name>
        <url-pattern>/ConfirmaReservaServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ConsultaDisponibilidadServlet</servlet-name>
        <url-pattern>/consultadisponibilidad</url-pattern>
    </servlet-mapping>


    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
  <welcome-file-list>
        <welcome-file> Login.jsp</welcome-file>
    </welcome-file-list>
    </web-app>
